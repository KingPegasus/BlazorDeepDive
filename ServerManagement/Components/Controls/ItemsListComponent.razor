@if (Items != null && Items.Count > 0)
{
    <MudList T="ToDoItem" SelectionMode="SelectionMode.MultiSelection">
        @foreach (var item in Items)
        {
            <ItemComponent Item="item"></ItemComponent>
        }
    </MudList>
}

@code {
    [Parameter]
    public List<ToDoItem>? Items { get; set; }
}
